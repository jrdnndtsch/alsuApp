<fieldset>
	<div class="field">
	  <%= f.label :media_type %><br>
	  <%= f.text_field :media_type %>
	</div>
	<div class="field">
	  <%= f.label :description %><br>
	  <%= f.text_area :description %>
	</div>
	<div class="field">
	  <%= f.label :video_link %><br>
	  <%= f.text_field :video_link %>
	</div>
	<% if @project.present? %>
		<div class="field">
		  <%= f.hidden_field :project_id, value: @project.id  %>
		</div>
	<% end %>
</fieldset>